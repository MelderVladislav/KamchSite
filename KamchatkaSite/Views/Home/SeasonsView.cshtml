@model IEnumerable<SeasonTable>
@{
    ViewBag.Title = "SeasonView";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>SeasonView</h2>
Здесь времена года

@foreach (SeasonTable season in Model)
{
    <div class="thumbs">
        <a href= @Url.RouteUrl(new { controller="Home",action="Service",seasonName=season.SeasonUrlName}) ><img class="seasonimage" src=@season.SeasonMainImage /></a>
        <div class="caption">
            <span class="title">Заголовок картинки</span>
            <span class="info">Краткое описание или анонс записи</span>
        </div>
    </div>
    
}
